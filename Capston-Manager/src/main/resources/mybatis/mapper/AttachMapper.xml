<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.jang.biz.mapper.AttachMapper">
  
  	<select id="getAttachList" resultType="AttachImageVO">
	
		SELECT * FROM goods_image WHERE kno = #{kno}
	
	</select>
	
	<select id="getAttachList2" resultType="MemberImageVO">
	
		SELECT * FROM member_image WHERE cno = #{cno}
	
	</select>
	
	<select id="getAttachList3" resultType="PlantImageVO">
	
		SELECT * FROM plant_image WHERE rno = #{rno}
	
	</select>


  </mapper>